	author = {Close, Hilary G. and Shah, Sunita R. and Ingalls, Anitra E. and Diefendorf, Aaron F. and Brodie, Eoin L. and Hansman, Roberta L. and Freeman, Katherine H. and Aluwihare, Lihini I. and Pearson, Ann},
	urldate = {2019-06-11},
	date = {2013-07-30},
	langid = {english},
	pmid = {23858459},
	keywords = {biogeochemistry, biomarkers, carbon isotopes, oceanography},
	file = {Full Text PDF:/home/millieginty/Zotero/storage/Z2BW7XMH/Close et al. - 2013 - Export of submicron particulate organic matter to .pdf:application/pdf;Snapshot:/home/millieginty/Zotero/storage/XLQ9BKGM/12565.html:text/html}
}

