	number = {6130},
	journaltitle = {Science},
	author = {Jaffé, Rudolf and Ding, Yan and Niggemann, Jutta and Vähätalo, Anssi V. and Stubbins, Aron and Spencer, Robert G. M. and Campbell, John and Dittmar, Thorsten},
	urldate = {2016-03-06},
	date = {2013-04-19},
	langid = {english},
	pmid = {23599492},
