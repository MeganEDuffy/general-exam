	shortjournal = {{PLoS} {ONE}},
	author = {Ghai, Rohit and Rodriguez-Valera, Francisco and {McMahon}, Katherine D. and Toyama, Danyelle and Rinke, Raquel and Cristina Souza de Oliveira, Tereza and Wagner Garcia, José and Pellon de Miranda, Fernando and Henrique-Silva, Flavio},
	date = {2011},
