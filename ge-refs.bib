	date = {2008-05-16},
	langid = {english},
	pmid = {18487183},
	file = {Full Text PDF:/home/millieginty/Zotero/storage/BFB4RGRG/Galloway et al. - 2008 - Transformation of the Nitrogen Cycle Recent Trend.pdf:application/pdf;Snapshot:/home/millieginty/Zotero/storage/GJ9FA6R3/889.html:text/html}
}

@article{codispoti_oceanic_2001,
	title = {The oceanic fixed nitrogen and nitrous oxide budgets: Moving targets as we enter the anthropocene?},
	rights = {Copyright [2001]. All efforts have been made to respect the copyright to the best of our knowledge. Inadvertent omissions, if brought to our notice, stand for correction and withdrawal of document from this repository.},
	url = {http://drs.nio.org/drs/handle/2264/1573},
	shorttitle = {The oceanic fixed nitrogen and nitrous oxide budgets},
	abstract = {New data forced to raise previous estimates of oceanic denitrification. The revised estimate of approx. 450 Tg N yr sup(-1) (Tg = 10 sup(12) g) produces an oceanic fixed N budget with a large deficit (approx. 200 Tg N yr sup(1)) that can be explained only by positing an ocean that has deviated far from a steady-state, the need for a major upwards revision of fixed N inputs, particularly nitrogen fixation, or both. Oceanic denitrification can be significantly altered by small re-distributions of carbon and dissolved oxygen. Since fixed N is a limiting nutrient, uncompensated changes in denitrification affect the ocean's ability to sequester atmospheric {CO} sub(2) via the "biological pump". Our concepts of the oceanic N sub(2) O regime to take better account of the extremely high N sub(2)O saturations that can arise in productive, low oxygen waters had to be modified. Recent results from the western Indian Shelf during a period when hypoxic, suboxic and anoxic waters were present produced a maximum surface N sub(2)O saturation of greater than 8000\%, a likely consequence of "stop and go" denitrification. The sensitivity of N sub(2) O production and consumption to small changes in the oceanic dissolved oxygen distribution and to the "spin-up" phase of denitrification suggests that the oceanic source term for N sub(2)O could change rapidly},
	author = {Codispoti, L. A. and Brandes, J. A. and Christensen, J. P. and Devol, A. H. and Naqvi, S. W. A. and Paerl, H. W. and Yoshinari, T.},
	urldate = {2016-12-16},
	date = {2001},
	langid = {english},
	file = {Snapshot:/home/millieginty/Zotero/storage/Q249HBTC/1573.html:text/html}
}

@article{kalvelage_aerobic_2015,
	title = {Aerobic Microbial Respiration In Oceanic Oxygen Minimum Zones},
	volume = {10},
	issn = {1932-6203},
	url = {http://journals.plos.org/plosone/article?id=10.1371/journal.pone.0133526},
	doi = {10.1371/journal.pone.0133526},
	abstract = {Oxygen minimum zones are major sites of fixed nitrogen loss in the ocean. Recent studies have highlighted the importance of anaerobic ammonium oxidation, anammox, in pelagic nitrogen removal. Sources of ammonium for the anammox reaction, however, remain controversial, as heterotrophic denitrification and alternative anaerobic pathways of organic matter remineralization cannot account for the ammonium requirements of reported anammox rates. Here, we explore the significance of microaerobic respiration as a source of ammonium during organic matter degradation in the oxygen-deficient waters off Namibia and Peru. Experiments with additions of double-labelled oxygen revealed high aerobic activity in the upper {OMZs}, likely controlled by surface organic matter export. Consistently observed oxygen consumption in samples retrieved throughout the lower {OMZs} hints at efficient exploitation of vertically and laterally advected, oxygenated waters in this zone by aerobic microorganisms. In accordance, metagenomic and metatranscriptomic analyses identified genes encoding for aerobic terminal oxidases and demonstrated their expression by diverse microbial communities, even in virtually anoxic waters. Our results suggest that microaerobic respiration is a major mode of organic matter remineralization and source of ammonium ({\textasciitilde}45-100\%) in the upper oxygen minimum zones, and reconcile hitherto observed mismatches between ammonium producing and consuming processes therein.},
	pages = {e0133526},
	number = {7},
	journaltitle = {{PLOS} {ONE}},
	shortjournal = {{PLOS} {ONE}},
	author = {Kalvelage, Tim and Lavik, Gaute and Jensen, Marlene M. and Revsbech, Niels Peter and Löscher, Carolin and Schunck, Harald and Desai, Dhwani K. and Hauss, Helena and Kiko, Rainer and Holtappels, Moritz and {LaRoche}, Julie and Schmitz, Ruth A. and Graco, Michelle I. and Kuypers, Marcel M. M.},
	urldate = {2016-10-24},
	date = {2015-07-20},
	keywords = {Oxygen, Oxidation, Aquatic respiration, Chlorophyll, Cytochrome oxidase, Oxygen consumption, Particle size, Respiration},
	file = {Full Text PDF:/home/millieginty/Zotero/storage/5SJVD9XF/Kalvelage et al. - 2015 - Aerobic Microbial Respiration In Oceanic Oxygen Mi.pdf:application/pdf;Snapshot:/home/millieginty/Zotero/storage/25I8XWUM/article.html:text/html}
}

@article{canfield_cryptic_2010,
	title = {A Cryptic Sulfur Cycle in Oxygen-Minimum–Zone Waters off the Chilean Coast},
	volume = {330},
	rights = {Copyright © 2010, American Association for the Advancement of Science},
	issn = {0036-8075, 1095-9203},
	url = {http://science.sciencemag.org/content/330/6009/1375},
	doi = {10.1126/science.1196889},
	abstract = {Cryptic Sulfur Cycling
	Aerobic bacteria and ocean circulation patterns control the formation and distribution of oxygen-minimum zones at moderate depth in the oceans. These habitats host microorganisms that thrive on other metabolic substrates in the absence of oxygen—most commonly, metabolizing thermodynamically favorable nitrogen compounds like nitrate. Off the coast of Chile, however, Canfield et al. (p. 1375, published online 11 November; see the Perspective by Teske) suggest that bacteria may often reduce sulfate as well. Metagenomic sequencing revealed the presence of both sulfate-reducing and sulfide-oxidizing bacteria. With the coincidence of sulfate and nitrate reduction, the sulfur and nitrogen cycles may be intimately linked; for example, sulfate reduction could provide nitrogen-rich ammonium for bacteria that ultimately transform it into nitrogen gas.
	Nitrogen cycling is normally thought to dominate the biogeochemistry and microbial ecology of oxygen-minimum zones in marine environments. Through a combination of molecular techniques and process rate measurements, we showed that both sulfate reduction and sulfide oxidation contribute to energy flux and elemental cycling in oxygen-free waters off the coast of northern Chile. These processes may have been overlooked because in nature, the sulfide produced by sulfate reduction immediately oxidizes back to sulfate. This cryptic sulfur cycle is linked to anammox and other nitrogen cycling processes, suggesting that it may influence biogeochemical cycling in the global ocean.
	Bacterial sulfur reduction and oxidation accompanies nitrogen cycling where oxygen levels at depth are low.
	Bacterial sulfur reduction and oxidation accompanies nitrogen cycling where oxygen levels at depth are low.},
	pages = {1375--1378},
	number = {6009},
	journaltitle = {Science},
	author = {Canfield, Don E. and Stewart, Frank J. and Thamdrup, Bo and Brabandere, Loreto De and Dalsgaard, Tage and Delong, Edward F. and Revsbech, Niels Peter and Ulloa, Osvaldo},
	urldate = {2016-05-12},
	date = {2010-12-03},
	langid = {english},
	pmid = {21071631},
	file = {Full Text PDF:/home/millieginty/Zotero/storage/MHZUE8B9/Canfield et al. - 2010 - A Cryptic Sulfur Cycle in Oxygen-Minimum–Zone Wate.pdf:application/pdf;Full Text PDF:/home/millieginty/Zotero/storage/WVRGS96X/Canfield et al. - 2010 - A Cryptic Sulfur Cycle in Oxygen-Minimum–Zone Wate.pdf:application/pdf;Snapshot:/home/millieginty/Zotero/storage/T2Q6KCZS/1375.html:text/html;Snapshot:/home/millieginty/Zotero/storage/4ZW58IZR/1375.html:text/html}
}

@article{carolan_transcriptomic_2015,
	title = {Transcriptomic evidence for microbial sulfur cycling in the eastern tropical North Pacific oxygen minimum zone},
	volume = {6},
	doi = {10.3389/fmicb.2015.00334},
	abstract = {Microbial communities play central roles in ocean biogeochemical cycles, and are particularly important in in oceanic oxygen minimum zones ({OMZs}). However, the key carbon, nitrogen, and sulfur (S) cycling processes catalyzed by {OMZ} microbial communities are poorly constrained spatially, temporally, and with regard to the different microbial groups involved. Here we sample across dissolved oxygen ({DO}) gradients in the oceans' largest {OMZ} by volume-the eastern tropical North Pacific ocean, or {ETNP}-and quantify 16S {rRNA} and functional gene transcripts to detect and constrain the activity of different S-cycling groups. Based on gene expression profiles, putative dissimilatory sulfite reductase ({dsrA}) genes are actively expressed within the {ETNP} {OMZ}. {dsrA} expression was limited almost entirely to samples with elevated nitrite concentrations, consistent with previous observations in the Eastern Tropical South Pacific {OMZ}. {dsrA} and 'reverse' dissimilatory sulfite reductase ({rdsrA}) genes are related and the associated enzymes are known to operate in either direction-reducing or oxidizing different S compounds. We found that {rdsrA} genes and {soxB} genes were expressed in the same samples, suggestive of active S cycling in the {ETNP} {OMZ}. These data provide potential thresholds for S cycling in {OMZs} that closely mimic recent predictions, and indicate that S cycling may be broadly relevant in {OMZs}.},
	pages = {334},
	journaltitle = {Frontiers in Microbiology},
	shortjournal = {Front Microbiol},
	author = {Carolan, Molly T. and Smith, Jason M. and Beman, J. M.},
	date = {2015},
	pmid = {26029168},
	pmcid = {PMC4426714},
	keywords = {sulfate reduction, Oxygen minimum zone, sulfur cyling, sulfur oxidation, transcriptomics}
}

@article{devries_marine_2013,
	title = {Marine denitrification rates determined from a global 3-D inverse model},
	volume = {10},
	issn = {1726-4189},
	url = {http://www.biogeosciences.net/10/2481/2013/},
	doi = {10.5194/bg-10-2481-2013},
	abstract = {A major impediment to understanding long-term changes in the marine nitrogen (N) cycle is the persistent uncertainty about the rates, distribution, and sensitivity of its largest fluxes in the modern ocean. We use a global ocean circulation model to obtain the first 3-D estimate of marine denitrification rates that is maximally consistent with available observations of nitrate deficits and the nitrogen isotopic ratio of oceanic nitrate. We find a global rate of marine denitrification in suboxic waters and sediments of 120–240 Tg N yr−1, which is lower than many other recent estimates. The difference stems from the ability to represent the 3-D spatial structure of suboxic zones, where denitrification rates of 50–77 Tg N yr−1 result in up to 50\% depletion of nitrate. This depletion reduces the effect of local isotopic enrichment on the rest of the ocean, allowing the N isotope ratio of oceanic nitrate to be achieved with a sedimentary denitrification rate about 1.3–2.3 times that of suboxic zones. This balance of N losses between sediments and suboxic zones is shown to obey a simple relationship between isotope fractionation and the degree of nitrate consumption in the core of the suboxic zones. The global denitrification rates derived here suggest that the marine nitrogen budget is likely close to balanced.},
	pages = {2481--2496},
	number = {4},
	journaltitle = {Biogeosciences},
	shortjournal = {Biogeosciences},
