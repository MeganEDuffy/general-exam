	issn = {1573-515X},
	url = {https://doi.org/10.1007/s10533-004-6546-9},
	doi = {10.1007/s10533-004-6546-9},
	abstract = {Proteinaceous components from four Washington coast margin sediments were extracted with base, fractionated into one of four size classes ({\textless}3 {kDa}, 3–10 {kDa}, 10–100 {kDa}, {\textgreater}100 {kDa}), and analyzed for their amino acid contents. Base-extracted material accounts for {\textasciitilde}30\% of the total hydrolyzable amino acids ({THAA}) and each size fraction has a unique composition, regardless of where the sediment was collected (shelf or upper slope). The {\textless}3 {kDa} size fraction ({\textasciitilde}10\% of base-extractable {THAA}) is relatively enriched in glycine ({\textasciitilde}30 mol\%), lysine ({\textasciitilde}5 mol\%), and non-protein amino acids ({\textasciitilde}5 mol\%). Glycine and non-protein amino acids are common degradation products, and lysine is very surface active. We suggest that the {\textless}3 {kDa} size fraction, therefore, represents a diagenetic mixture of fragments produced during the degradation of larger proteins. The 3–10 and 10–100 {kDa} size fractions ({\textasciitilde}10\% and 42\% of base-extractable {THAA}, respectively) have similar amino acid distributions dominated by aspartic acid ({\textasciitilde}30 mol\%). Enrichments in Asp is likely due to both preservation of Asp-rich proteins and the production of Asp during degradation. The {\textgreater}100 {kDa} size fraction ({\textasciitilde}38\% of base-extractable {THAA}) is not dominated by any particular amino acid and can not be modeled by mixing the amino acid compositions of the other size fractions. We propose that the larger size fractions (10–100 {kDa} and {\textgreater}100 {kDa}) represent intact, or near intact, proteins. Estimates of isoelectric points and relative hydrophobicity suggest the base-extractable proteins are primarily acidic and have globular structures. Statistical comparisons to several known proteins indicates that the base-extractable component is most similar to planktonic cytoplasmic proteins.},
	pages = {177--200},
	number = {2},
	journaltitle = {Biogeochemistry},
	shortjournal = {Biogeochemistry},
	author = {Nunn, Brook L. and Keil, Richard G.},
	urldate = {2018-11-21},
	date = {2005-08-01},
	langid = {english},
	keywords = {Protein, Amino acids, Diagenesis, Isoelectric point, Marine sediment},
	file = {Springer Full Text PDF:/home/millieginty/Zotero/storage/4884SVQE/Nunn and Keil - 2005 - Size Distribution and Amino Acid Chemistry of Base.pdf:application/pdf}
}

@article{braun_microbial_2017,
	title = {Microbial turnover times in the deep seabed studied by amino acid racemization modelling},
	volume = {7},
	issn = {2045-2322},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5516024/},
	doi = {10.1038/s41598-017-05972-z},
	abstract = {The study of active microbial populations in deep, energy-limited marine sediments has extended our knowledge of the limits of life on Earth. Typically, microbial activity in the deep biosphere is calculated by transport-reaction modelling of pore water solutes or from experimental measurements involving radiotracers. Here we modelled microbial activity from the degree of D:L-aspartic acid racemization in microbial necromass (remains of dead microbial biomass) in sediments up to ten million years old. This recently developed approach (D:L-amino acid modelling) does not require incubation experiments and is highly sensitive in stable, low-activity environments. We applied for the first time newly established constraints on several important input parameters of the D:L-amino acid model, such as a higher aspartic acid racemization rate constant and a lower cell-specific carbon content of sub-seafloor microorganisms. Our model results show that the pool of necromass amino acids is turned over by microbial activity every few thousand years, while the turnover times of vegetative cells are in the order of years to decades. Notably, microbial turnover times in million-year-old sediment from the Peru Margin are up to 100-fold shorter than previous estimates, highlighting the influence of microbial activities on element cycling over geologic time scales.},
	journaltitle = {Scientific Reports},
	shortjournal = {Sci Rep},
	author = {Braun, Stefan and Mhatre, Snehit S. and Jaussi, Marion and Røy, Hans and Kjeldsen, Kasper U. and Pearce, Christof and Seidenkrantz, Marit-Solveig and Jørgensen, Bo Barker and Lomstein, Bente Aa.},
	urldate = {2018-06-25},
	date = {2017-07-18},
	pmid = {28720809},
	pmcid = {PMC5516024},
	file = {PubMed Central Full Text PDF:/home/millieginty/Zotero/storage/R9M7MFXV/Braun et al. - 2017 - Microbial turnover times in the deep seabed studie.pdf:application/pdf}
}

@article{bergauer_organic_2017,
	title = {Organic matter processing by microbial communities throughout the Atlantic water column as revealed by metaproteomics},
