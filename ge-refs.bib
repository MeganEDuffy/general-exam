	number = {1},
	journaltitle = {Marine Drugs},
	shortjournal = {Mar Drugs},
	author = {Bose, Utpal and Hewavitharana, Amitha K. and Ng, Yi Kai and Shaw, Paul Nicholas and Fuerst, John A. and Hodson, Mark P.},
