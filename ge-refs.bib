	langid = {english},
	keywords = {biochar, black carbon, Decomposition, Dissolved black carbon, Ecology, Plant Physiology, Plant Sciences, Simulation model, Soil Science \& Conservation},
	file = {Full Text PDF:/home/millieginty/Zotero/storage/RBK3F5P2/Foereid et al. - 2011 - Modeling black carbon degradation and movement in .pdf:application/pdf;Snapshot:/home/millieginty/Zotero/storage/CF9FGRNM/10.html:text/html}
