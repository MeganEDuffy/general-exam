	file = {ACS Full Text PDF w/ Links:/home/millieginty/Zotero/storage/Q7SKU87E/Short et al. - 2004 - Estimate of Oil Persisting on the Beaches of Princ.pdf:application/pdf;ACS Full Text Snapshot:/home/millieginty/Zotero/storage/VEMWPD5F/es0348694.html:text/html}
}

@article{atlas_oil_2011,
	title = {Oil Biodegradation and Bioremediation: A Tale of the Two Worst Spills in U.S. History},
