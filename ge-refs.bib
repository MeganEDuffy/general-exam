	pmcid = {PMC3806317},
	file = {PubMed Central Full Text PDF:/home/millieginty/Zotero/storage/4LHTNF9B/Milac et al. - 2012 - Analyzing LC-MSMS data by spectral count and ion .pdf:application/pdf}
}

@article{keil_multiproxy_2016,
	title = {A multiproxy approach to understanding the "enhanced" flux of organic matter through the oxygen-deficient waters of the Arabian Sea},
	volume = {13},
	issn = {1726-4170},
	url = {https://www.biogeosciences.net/13/2077/2016/},
	doi = {https://doi.org/10.5194/bg-13-2077-2016},
	abstract = {{\textless}p{\textgreater}{\textless}strong{\textgreater}Abstract.{\textless}/strong{\textgreater} Free-drifting sediment net traps were deployed 14 times at depths between 80 and 500{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}m for 1–3 days each during the late monsoon–intermonsoon transition in the central Arabian Sea. Two locations (19.5 and 15.5°{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}N) were within the permanently oxygen-deficient zone ({ODZ}), and a third (11°{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}N) had a shallow and thin oxygen minimum. The secondary nitrite maximum, which serves as a tracer of the {ODZ}, thinned from ∼ 250{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}m thick at stations 19.5 and 15.5°{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}N to ∼ 50{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}m thick at station 11°{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}N. Overall, organic carbon fluxes ranged from 13.2{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}g{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}m$^{\textrm{2}}${\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}yr$^{\textrm{−1}}$ at 80{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}m to a minimum of 1.1{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}g{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}m$^{\textrm{2}}${\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}yr$^{\textrm{−1}}$ at 500{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}m. Fluxes at the more oxygenated 11°{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}N station attenuate faster than within the permanent {ODZ}. Martin curve attenuation coefficients for 19.5 and 15.5°{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}N are respectively 0.59 and 0.63 and for 11°{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}N it is 0.98. At least six potential mechanisms might explain why particles sinking through the {ODZ} are more effectively transferred to depth: (M1) oxygen effects, (M2) microbial loop efficiencies and chemoautotrophy, (M3) changes in zooplankton dynamics, (M4) additions of ballast that might sorb and protect organic matter from decay (M4a) or change sinking speeds (M4b), (M5) inputs of refractory organic matter and (M6) temperature effects. These mechanisms are intertwined, and they were explored using a combination of mineral ({XPS}) and organic matter characterizations of the sinking material, shipboard incubation experiments, and evaluations of existing literature. Direct evidence was found supporting an oxygen effect and/or changes in the efficiency of the microbial loop including the addition of chemoautotrophic carbon to the sinking flux in the upper 500{\textless}span class="thinspace"{\textgreater}{\textless}/span{\textgreater}m. Less direct evidence was found for the other potential mechanisms. A simple conceptual model consistent with our and other recent data suggests that the upper {ODZ} microbial community determines the initial flux attenuation, and that zooplankton and sinking speed become more important deeper in the water column. The exact interplay between the various mechanisms remains to be further evaluated.{\textless}/p{\textgreater}},
	pages = {2077--2092},
	number = {7},
	journaltitle = {Biogeosciences},
	author = {Keil, Richard G. and Neibauer, Jacquelyn A. and Biladeau, Christina and van der Elst, Kelsey and Devol, Allan H.},
	urldate = {2019-07-29},
	date = {2016-04-08},
	file = {Full Text PDF:/home/millieginty/Zotero/storage/P4MQZCTJ/Keil et al. - 2016 - A multiproxy approach to understanding the enhanc.pdf:application/pdf;Snapshot:/home/millieginty/Zotero/storage/D6JKZAE6/2016.html:text/html}
}

