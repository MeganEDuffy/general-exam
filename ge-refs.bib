	langid = {english},
	file = {Ho et al. - 2018 - Unification of Protein Abundance Datasets Yields a.pdf:/home/millieginty/Zotero/storage/IPY6FH3T/Ho et al. - 2018 - Unification of Protein Abundance Datasets Yields a.pdf:application/pdf}
}

