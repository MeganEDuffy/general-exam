@article{tschager_better_2017,
	title = {A better scoring model for de novo peptide sequencing: the symmetric difference between explained and measured masses},
	volume = {12},
	issn = {1748-7188},
	url = {https://doi.org/10.1186/s13015-017-0104-1},
	doi = {10.1186/s13015-017-0104-1},
	shorttitle = {A better scoring model for de novo peptide sequencing},
	abstract = {Given a peptide as a string of amino acids, the masses of all its prefixes and suffixes can be found by a trivial linear scan through the amino acid masses. The inverse problem is the ideal                            de novo                            peptide sequencing problem: Given all prefix and suffix masses, determine the string of amino acids. In biological reality, the given masses are measured in a lab experiment, and measurements by necessity are noisy. The (real, noisy) de novo peptide sequencing problem therefore has a noisy input: a few of the prefix and suffix masses of the peptide are missing and a few other masses are given in addition. For this setting, we ask for an amino acid string that explains the given masses as accurately as possible.},
	pages = {12},
	journaltitle = {Algorithms for Molecular Biology},
	shortjournal = {Algorithms for Molecular Biology},
	author = {Tschager, Thomas and Rösch, Simon and Gillet, Ludovic and Widmayer, Peter},
	urldate = {2017-07-25},
	date = {2017-05-11},
	keywords = {Mass Spectrometry, Computational proteomics, De novo peptide sequencing, Peptide identification},
	file = {Full Text PDF:/home/millieginty/Zotero/storage/W7J745Q9/Tschager et al. - 2017 - A better scoring model for de novo peptide sequenc.pdf:application/pdf;Snapshot:/home/millieginty/Zotero/storage/3XCX7EJM/s13015-017-0104-1.pdf:application/pdf}
}

