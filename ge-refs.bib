	shortjournal = {Limnol. Oceanogr.},
	author = {Grutters, Mark and van Raaphorst, Wim and Epping, Eric and Helder, Willem and de Leeuw, Jan W. and Glavin, Daniel P. and Bada, Jeffrey},
	urldate = {2016-03-17},
	date = {2002-09-01},
	langid = {english},
	file = {Full Text PDF:/home/millieginty/Zotero/storage/HDHEIMHP/Grutters et al. - 2002 - Preservation of amino acids from in situ-produced .pdf:application/pdf;Snapshot:/home/millieginty/Zotero/storage/MFVMQF2K/abstract.html:text/html}
