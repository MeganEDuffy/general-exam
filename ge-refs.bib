	keywords = {Applications, Bioemulsifier, Biosurfactant, Exopolysaccharides, Glycolipids, Lipopeptides, Marine},
	file = {ScienceDirect Full Text PDF:/home/millieginty/Zotero/storage/A866H7QZ/Satpute et al. - 2010 - Biosurfactants, bioemulsifiers and exopolysacchari.pdf:application/pdf;ScienceDirect Snapshot:/home/millieginty/Zotero/storage/PGWQVMZR/S0734975010000261.html:text/html}
