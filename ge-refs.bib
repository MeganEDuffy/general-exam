	pmid = {22101058},
	pmcid = {PMC3264114},
	file = {PubMed Central Full Text PDF:/home/millieginty/Zotero/storage/F7S4Q895/Köllner et al. - 2012 - Bacterial Chitin Hydrolysis in Two Lakes with Cont.pdf:application/pdf}
}

